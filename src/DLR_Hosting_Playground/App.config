				  type="Microsoft.JScript.Runtime.JSContext, Microsoft.JScript.Runtime, Version=1.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />
      <language names="IronScheme;Scheme;ss"
                extensions=".ss"
                displayName="IronScheme 1.0 Beta"
